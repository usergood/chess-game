{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ES2022",
      "moduleResolution": "bundler",                     
      "paths": {
        "wasi:http/types@0.2.1": ["./types/interfaces/wasi-http-types.d.ts" ],
        "wasi:clocks/monotonic-clock@0.2.1": ["./types/interfaces/wasi-clocks-monotonic-clock.d.ts"],
        "wasi:clocks/wall-clock@0.2.1": ["./types/interfaces/wasi-clocks-wall-clock.d.ts"],
        "wasi:random/random@0.2.1": ["./types/interfaces/wasi-random-random.d.ts"],
        "wasi:cli/environment@0.2.1": ["./types/interfaces/wasi-cli-environment.d.ts"],
      },                                   
      "outDir": "./dist/",                                  
      "esModuleInterop": true,                             
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true                                 
    },
    "include": ["src"]
  }